
# "prescale: N" means, update the plots every Nth event 

prescale: 200


# If "digital_sum_only" is "true" as opposed to "false", only show the
#  histograms, not the event snapshots

digital_sum_only: false

# Can set the "num_x_plots" and "num_y_plots" variables to define the
#  layout of plots within the canvas

#num_x_plots: 3
#num_y_plots: 1

%{enable_onmon} write_to_file: %{write_to_file}
%{enable_onmon} %{onmon_fileName}

